{
  "expo": {
    "name": "HOF Golf",
    "slug": "hof-golf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hofgolf",
    "userInterfaceStyle": "automatic",
    "ios": {
      "icon": "./assets/ios-light.png",
      "supportsTablet": false,
      "bundleIdentifier": "com.keithkurak.hofgolf",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3F2F8F",
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "predictiveBackGestureEnabled": false,
      "package": "com.keithkurak.hofgolf",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#208AEF",
          "android": {
            "image": "./assets/images/splash-icon.png",
            "imageWidth": 76
          }
        }
      ],
      "expo-sqlite",
      "expo-sharing"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b4b5b473-8c41-43da-a8ac-773e0bf35423"
      }
    },
    "owner": "expo-production",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b4b5b473-8c41-43da-a8ac-773e0bf35423"
    }
  }
}
